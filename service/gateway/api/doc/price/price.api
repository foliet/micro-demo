type Item {
    ItemId int64 `json:"itemId"`
    ShopId int64 `json:"shopId,optional"`
}

type ItemInfo {
    ItemId  int64   `json:"itemId"`
    CreateAt int64  `json:"createAt"`
    Price   float64 `json:"price"`
}

type ItemInfos {
    ItemInfos []*ItemInfo `json:"itemInfos"`
}

@server (
    jwt: Auth
    group: price
    prefix: /api/price
)
service gateway {
    @handler subscribe
    post /subscribe (Item)
    @handler itemInfo
    get /iteminfo returns (ItemInfos)
}